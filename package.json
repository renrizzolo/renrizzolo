{
  "name": "renrizzolo",
  "private": true,
  "version": "0.1.0",
  "description": "Ren Rizzolo",
  "types": "src/components.d.ts",
  "scripts": {
    "build": "stencil build --prod --prerender",
    "start": "stencil build --dev --watch --serve",
    "start.prod": "stencil build --prod --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "start.server": "npm run start --workspace=server",
    "build-blog": "npm run build --workspace=blog",
    "deploy": "npm run build-blog && npm run build"
  },
  "dependencies": {
    "@stencil/core": "^1.2.4",
    "@stencil/router": "1.0.1",
    "lottie-web": "^5.5.5"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/postcss": "^1.0.1",
    "@types/autoprefixer": "^9.5.0",
    "@types/jest": "24.0.15",
    "@types/puppeteer": "1.12.4",
    "autoprefixer": "^9.6.0",
    "jest": "24.8.0",
    "jest-cli": "24.9.0",
    "puppeteer": "1.19.0"
  },
  "overrides": {
    "@types/babel__traverse": "7.0.6"
  },
  "workspaces": [
    "server",
    "blog"
  ]
}
